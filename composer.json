{
  "name": "wpn/starter-theme",
  "type": "project",
  "description": "The WordPress Neon Framework.",
  "keywords": [
    "framework",
    "wordpress-neon"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1.0",
    "wpn/framework": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "App\\": "inc/"
    },
    "exclude-from-classmap": [
      "inc/config"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "post-create-project-cmd": []
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}